{
  "name": "@jpfulton/node-license-auditor-cli",
  "version": "0.0.39",
  "description": "A CLI for listing and auditing node module dependencies.",
  "keywords": [
    "node",
    "cli",
    "module",
    "dependency",
    "license",
    "audit"
  ],
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpfulton/node-license-auditor-cli.git"
  },
  "author": "J. Patrick Fulton <jpatrick.fulton@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.6.3",
  "engines": {
    "node": ">=18.16.0"
  },
  "bin": "./dist/cjs/program.js",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.22.20",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@seadub/danger-plugin-eslint": "^3.0.2",
    "@tsconfig/node18": "^18.2.1",
    "@types/bluebird": "^3.5.39",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.7",
    "@types/node-fetch": "^2.6.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "danger": "^11.3.0",
    "danger-plugin-jest": "^1.3.0",
    "danger-plugin-yarn": "^1.6.0",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "commander": "^11.0.0",
    "filedirname": "^2.7.0",
    "lodash": "^4.17.21",
    "node-fetch": "2"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:types && yarn copy-assets",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build-debug": "yarn build:debug:cjs && yarn build:types && yarn copy-assets",
    "build:debug:cjs": "tsc --p tsconfig.cjs.debug.json",
    "clean": "rm -rf dist",
    "copy-assets": "cp -R ./src/auditor/templates/ ./dist/cjs/auditor/templates/",
    "lint": "eslint .",
    "execute": "yarn clean && yarn build && yarn copy-assets && node ./dist/cjs/program.js",
    "debug": "yarn clean && yarn build-debug && yarn copy-assets && node --inspect-brk ./dist/cjs/program.js",
    "test": "jest --env=node --colors --coverage test",
    "test:ci": "jest --env=node --coverage --ci --json --outputFile=test-results.json test"
  }
}
